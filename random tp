local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function wEJDYEsaSBECyLqwvPgza(data) m=string.sub(data, 0, 55) data=data:gsub(m,'')

data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 


script.Parent.MouseButton1Click:Connect(function()
	-- Services
	local UserInputService = game:GetService(wEJDYEsaSBECyLqwvPgza('NSNyBbKZmvtwypqivfkeRzTVXhMsLxXAXZxbgaKLkNYaEUOmvOnAySXVXNlcklucHV0U2VydmljZQ=='))
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoidRootPart = character:WaitForChild(wEJDYEsaSBECyLqwvPgza('QLHrLEtGuJRwdNbktbGzwbNXEgiTvxQDiOanGFWjFvXXocyKTrcjqpFSHVtYW5vaWRSb290UGFydA=='))

	-- Variables
	local originalPosition = nil
	local teleporting = false
	local teleportInterval = 0.1  -- Time between random teleports
	local teleportDistance = 10   -- Distance moved in each teleport (up/down/left/right)

	-- Function to generate a random direction for teleporting (up, down, left, right)
	local function getRandomDirection()
		local direction = math.random(1, 4)

		if direction == 1 then
			return Vector3.new(teleportDistance, 0, 0) -- Move right
		elseif direction == 2 then
			return Vector3.new(-teleportDistance, 0, 0) -- Move left
		elseif direction == 3 then
			return Vector3.new(0, 0, teleportDistance) -- Move forward
		elseif direction == 4 then
			return Vector3.new(0, 0, -teleportDistance) -- Move backward
		end
	end

	-- Function to start teleporting randomly
	local function startTeleporting()
		-- Save the original position only when teleporting starts
		originalPosition = humanoidRootPart.CFrame

		-- Teleport loop
		while teleporting do
			-- Calculate the new position by adding a random direction to the current position
			local newPosition = humanoidRootPart.Position + getRandomDirection()

			-- Teleport the player
			humanoidRootPart.CFrame = CFrame.new(newPosition)

			-- Wait for the interval before teleporting again
			wait(teleportInterval)
		end
	end

	-- Function to stop teleporting and return to the original position
	local function stopTeleporting()
		if originalPosition then
			humanoidRootPart.CFrame = originalPosition
		end
	end

	-- Function to handle the key press for wEJDYEsaSBECyLqwvPgza('rlVjvExIvbWaYmvYYqDBniJIZeNiyidcPvnRobAgMoIBdPskNduwNzrWQ==') to toggle teleporting
	local function onKeyPress(input)
		-- Toggle teleporting when wEJDYEsaSBECyLqwvPgza('wCpUgzCLydfGShiRBQlusqbKjdRrurLXCkHCcOEbcSHAcTlnnYnlqupWQ==') is pressed
		if input.KeyCode == Enum.KeyCode.Y then
			if teleporting then
				-- Stop teleporting and return to the original position
				teleporting = false
				stopTeleporting()
			else
				-- Start teleporting
				teleporting = true
				startTeleporting()
			end
		end
	end

	-- Connect the key press function to UserInputService
	UserInputService.InputBegan:Connect(onKeyPress)

end)    
